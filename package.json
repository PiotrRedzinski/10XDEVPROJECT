{
  "name": "genfish",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "npm-run-all -p build:*",
    "build:astro": "astro build",
    "build:css": "sass src/:src/ --style=compressed",
    "build:worker": "esbuild ./src/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "dev:e2e": "astro dev --mode test",
    "pages:deploy": "astro build && wrangler pages deploy ./dist"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.5.3",
    "@astrojs/node": "^9.2.2",
    "@astrojs/react": "^3.0.0",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.11",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "astro": "^5.8.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lightningcss": "1.24.1",
    "lucide-react": "^0.487.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "3.4.1",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/prop-types": "^15.7.14",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.2.0",
    "happy-dom": "^17.4.4",
    "husky": "9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "15.5.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-astro": "0.14.1",
    "sass": "^1.71.0",
    "supabase": "^2.22.6",
    "typescript-eslint": "8.28.0",
    "vitest": "^3.1.2",
    "wrangler": "^4.16.1"
  },
  "lint-staged": {
    "*.{ts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "description": "10xproject",
  "main": "eslint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PiotrRedzinski/10XDEVPROJECT.git"
  },
  "author": "Piotr Redzinski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PiotrRedzinski/10XDEVPROJECT/issues"
  },
  "homepage": "https://github.com/PiotrRedzinski/10XDEVPROJECT#readme"
}
